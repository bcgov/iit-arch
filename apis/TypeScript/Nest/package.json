{
  "name": "nest-js-crud-api",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript repository for CRUD",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "nest build",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev:linux": "npm run clean && npm run build && export NODE_OPTIONS=--max_old_space_size=250 && nest start --watch",
    "start:dev:windows": "npm run clean && npm run build && set NODE_OPTIONS=--max_old_space_size=250 && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.17",
    "@nestjs/common": "^9.0.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.3",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/swagger": "^6.0.3",
    "@nestjs/terminus": "^9.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "apollo-server-express": "^3.8.2",
    "csurf": "^1.11.0",
    "graphql": "^16.5.0",
    "helmet": "^5.1.0",
    "keycloak-connect": "^18.0.2",
    "nest-keycloak-connect": "^1.9.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.0.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "ncp": "^2.0.0",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.6",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
